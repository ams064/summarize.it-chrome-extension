{
    "manifest_version": 2,

    "name": "Summarize.it",
    "description": "Generate Summaries in 1-click. A chrome extension from Summarize.it",
    "version": "1.0.2",
    "icons": {
        "16": "favicon.png",
        "48": "favicon.png",
        "128": "favicon.png"
    },

    "permissions": [
        "activeTab",
		"tabs"
    ],
    "browser_action": {
        "default_icon": "favicon.png",
        "default_popup": "popup.html"
    }
}
